<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - DemoCedeDigital</title>

    <link rel="apple-touch-icon" href="images/logo_banorte_movil.png">
    <link rel="shortcut icon" href="images/logo_banorte_movil.png">

    <link rel="stylesheet" href="css/lib/normalize/normalize.min.css">
    <link rel="stylesheet" href="css/lib/bootstrap/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/font-awesome@4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/lykmapipo/themify-icons@0.1.2/css/themify-icons.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/pixeden-stroke-7-icon@1.2.3/pe-icon-7-stroke/dist/pe-icon-7-stroke.min.css">
    <link rel="stylesheet" href="css/lib/flag-icon/flag-icon.css">
    <link rel="stylesheet" href="css/cs-skin-elastic.css">
    <link rel="stylesheet" href="css/style.css">

    <link href="~/css/lib/widgets/chartist.min.css" rel="stylesheet">
    <link href="~/css/lib/widgets/jqvmap.min.css" rel="stylesheet">

    <link href="~/css/lib/widgets/weather-icons.css" rel="stylesheet" />
    <link href="~/css/lib/widgets/fullcalendar.min.css" rel="stylesheet" />

    <!--SweetAlert-->
    <script src="js/lib/sweetalert/sweetalert.js"></script>

    <link href='css/lib/fonts/fonts.css' rel='stylesheet' type='text/css'>

    <script src="js/lib/jQuery/jquery.min.js"></script>
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <aside id="left-panel" class="left-panel">
        <nav class="navbar navbar-expand-sm navbar-default">
            <div id="main-menu" class="main-menu collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li class="menu-title">Menu</li><!-- /.menu-title -->
                    <li id="liidLista">
                    </li>
                </ul>
            </div><!-- /.navbar-collapse -->
        </nav>
    </aside>
    <div id="right-panel" class="right-panel">
        <!-- Header-->
        <header id="header" class="header">
            <div class="top-left">
                <div class="navbar-header">
                    <a class="navbar-brand" href="./"><img src="images/logo_banorte.png" alt="Logo"></a>
                    <a class="navbar-brand hidden" href="./"><img src="images/logo_banorte.png" alt="Logo"></a>
                    <a id="menuToggle" class="menutoggle"><i class="fa fa-bars"></i></a>
                </div>
            </div>
            <div class="top-right">
                <div class="header-menu">
                    <div class="header">
                        <div style="margin-top:10px">
                            <span class="nav-link"><strong id="stroCategoriaUser"></strong></span>
                        </div>
                    </div>

                    <div class="header">
                        <div style="margin-top:10px">
                            <span id="stroNombre" class="nav-link"></span>
                        </div>
                    </div>

                    <div class="header-left">
                        <div class="dropdown for-notification">
                            <button class="btn btn-secondary dropdown-toggle" type="button" id="notification" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <i class="fa fa-bell"></i>
                                <span class="count bg-danger">3</span>
                            </button>
                            <div class="dropdown-menu" aria-labelledby="notification">
                                <p class="red">Tienes 3 notificaciones</p>
                                <a class="dropdown-item media" href="#">
                                    <i class="fa fa-check"></i>
                                    <p>Solicitud de liberación autorizada.</p>
                                </a>
                                <a class="dropdown-item media" href="#">
                                    <i class="fa fa-info"></i>
                                    <p>Visita de Inspección rechazada.</p>
                                </a>
                                <a class="dropdown-item media" href="#">
                                    <i class="fa fa-warning"></i>
                                    <p>Pago Autorizado.</p>
                                </a>
                            </div>
                        </div>
                    </div>



                    <div class="user-area dropdown float-right">
                        <a href="#" class="dropdown-toggle active" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <img class="user-avatar rounded-circle" id="imgUsuario" alt="Avatar de usuario">
                        </a>

                        <div class="user-menu dropdown-menu">


                            <a class="nav-link" href="#"><i class="fa fa-user"></i>Mi perfil</a>

                            <a class="nav-link" href="#"><i class="fa fa-cog"></i>Billetera</a>

                            <a class="nav-link" href="~/Login"><i class="fa fa-power-off"></i>Cerrar sesión</a>
                        </div>
                    </div>

                </div>
            </div>
        </header>
    </div>
    @RenderBody()

    <!--<script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>-->
    <script src="js/lib/popper/popper.min.js"></script>
    <script src="~/js/lib/bootstrap/bootstrap.min.js"></script>
    <script src="~/js/lib/jQuery/jquery.matchHeight.min.js"></script>
    <script src="js/main.js"></script>

    <!--  Chart js -->
    <script src="~/js/lib/chartjs/chart.bundle.min.js"></script>

    <!--Chartist Chart-->
    <script src="~/js/lib/chartist/chartist.min.js"></script>
    <script src="~/js/lib/chartist/chartist-plugin-legend.min.js"></script>

    <script src="~/js/lib/jQuery/jquery.flot.min.js"></script>
    <script src="~/js/lib/jQuery/jquery.flot.pie.min.js"></script>
    <script src="~/js/lib/jQuery/jquery.flot.spline.min.js"></script>

    <script src="~/js/lib/jQuery/jquery.simpleWeather.min.js"></script>
    <script src="js/init/weather-init.js"></script>
    

    <script src="~/js/lib/moment/moment.min.js"></script>
    <script src="~/js/lib/fullcalendar/fullcalendar.min.js"></script>
    <script src="js/init/fullcalendar-init.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
